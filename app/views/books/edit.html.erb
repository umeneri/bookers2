<h2>Editing Book</h2>
<%= render  'books/errors', book: @book %>
<%= form_with model:@book,local:true do |f| %>
  <label for="inputTitle">Title</label>
  <%= f.text_field :title, autofocus: true, id:"inputTitle", placeholder:"title"%>
  <label for="inputBody">Opinion</label>
  <%= f.text_area :body, autofocus: false, id:"inputBody", placeholder:"body"%>
  <%= f.submit "変更を保存" %>
<% end %>